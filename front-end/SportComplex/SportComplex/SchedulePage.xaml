<Page x:Class="SportComplex.SchedulePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SportComplex"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SchedulePage">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/РасписаниеЗаявок.png"/>
        </Grid.Background>
        <Button Content="Button" HorizontalAlignment="Left" Margin="1111,310,0,0" Width="100" Height="35" VerticalAlignment="Top" Background="#FFDDDDDD" Opacity="0" Click="ButtonMoreDetails_Click"/>
        <Image Grid.Column="1" HorizontalAlignment="Left" Height="28" Margin="22,600,0,0" VerticalAlignment="Top" Width="26" Source="/logout.png" Stretch="Uniform"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="28" Margin="22,600,0,0" VerticalAlignment="Top" Width="26"  Grid.Column="1" Opacity="0" Click="ButtonLogout_Click"/>
    </Grid>
</Page>
